<%- include('Head') %>
<%- include('Navigation') %>
        <!-- Header-->
        <header class="bg-dark py-5">
            <div class="container px-4 px-lg-5 my-4">
            </div>
        </header>
        <body class="bg-gradient-primary">

            <div class="container">
        
                <!-- Outer Row -->
                <div class="row justify-content-center">
        
                    <div class="col-xl-10-custom col-lg-12 col-md-9">
        
                        <div class="card o-hidden border-0 shadow-lg my-5">
                            <div class="card-body p-0">
                                <!-- Nested Row within Card Body -->
                                <div class="row">
                                    <div class="col-lg-6 d-none d-lg-block bg-login-image">
                                        <img style="width:400px" class="card-img-top" src="/assets/img/1x1 Logo.png" alt="..." />
                                    </div>
                                    <!--Login stuff-->
                                    <div class="col-lg-6">
                                        <div class="p-5">
                                            <div class="text-center">
                                                <h1 class="h4 text-gray-900 mb-4">Welcome Back Chelsey!</h1>
                                            </div>
                                            <form class="user"
                                                action="/userLogin"
                                                method="post">
                                                <div>
                                                    <input type="email" class="form-control form-control-user"
                                                        id="useremail" aria-describedby="emailHelp"
                                                        placeholder="Enter Email Address...">
                                                        <br>
                                                </div>
                                                <div class="form-group">
                                                    <input type="userpassword" class="form-control form-control-user"
                                                        id="exampleInputPassword" placeholder="Password">
                                                </div>
                                                <br>
                                                <div style="text-align: center;">
                                                <a href="index.html" class="button" style="width:250px">
                                                    Login
                                                </a>
                                                </div>
                                            </form>
                                            <hr>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
        
                    </div>
        
                </div>
        
            </div>
        
            <!-- Bootstrap core JavaScript-->
            <script src="vendor/jquery/jquery.min.js"></script>
            <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
        
            <!-- Core plugin JavaScript-->
            <script src="vendor/jquery-easing/jquery.easing.min.js"></script>
        
            <!-- Custom scripts for all pages-->
            <script src="js/sb-admin-2.min.js"></script>
        
<%- include('Footer') %>
