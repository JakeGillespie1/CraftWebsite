<%- include('Head') %> <%- include('Header') %>

<section onload="gatherLog();">
    <h1>
        Successfully Logged In
      </h1>
    <p id="logged_in" style="display: none;"><%= reviewData[0].logged_in %></p>
<script>
    // Wait for 1000 milliseconds (1 second) before redirecting
    setTimeout(function() {
      location.href = "https://craftwebsite.is404.net";
    }, 1000);

    function gatherLog()
    {
        localStorage.setItem(loggedIn, document.getElementById("logged_in").innerHTML);
    }
</script>
</section>

